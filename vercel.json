{
	"rewrites": [
		{
			"source": "/case-1",
			"has": [
				{
					"type": "host",
					"value": "test\\.vercel-support\\.app"
				}
			],
			"destination": "/user/_user_id"
		},
		{
			"source": "/case-2",
			"destination": "https://nuxtjs.org/"
		},
		{
			"source": "/case-3",
			"has": [
				{
					"type": "host",
					"value": "(?<sub>.*)\\.vercel-support\\.app"
				}
			],
			"destination": "https://which-slug.vercel-support.app/:sub"
		},
		{
			"source": "/case-4",
			"has": [
				{
					"type": "host",
					"value": "(?<sub>.*)\\.vercel-support\\.app"
				}
			],
			"destination": "https://mini-nuxt.vercel.app/user/:sub"
		}
  ],
	"trailingSlash": false
}