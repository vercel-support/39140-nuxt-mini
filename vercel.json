{
	"rewrites": [
		{
			"source": "/test",
			"has": [
				{
					"type": "host",
					"value": "test\\.vercel-support\\.app"
				}
			],
			"destination": "/user/_user_id"
		},
		{
			"source": "/nuxt",
			"destination": "https://nuxtjs.org/"
		},
		{
			"source": "/host-rewrite-example",
			"has": [
				{
					"type": "host",
					"value": "(?<sub>.*)\\.vercel-support\\.app"
				}
			],
			"destination": "https://mini-nuxt.vercel.app/user/:sub"
		}
  ],
	"trailingSlash": false
}